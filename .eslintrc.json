{
  "env": {
      "browser": true,
      "es2021": true,
      "node": true
  },
  "extends":
  [
    "plugin:cypress/recommended",
    "google"
  ],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "rules": {
    "indent": [
      "error",
      2
  ],
  "quotes": [
      "error",
      "single"
  ],
  "semi": [
      "error",
      "never"
  ],
  "no-trailing-spaces": [
      "error"
  ],
  "require-jsdoc":"off",
  "max-len": ["error", { "code": 200 }],
  "cypress/no-force": "error",
  "cypress/assertion-before-screenshot": "error",
  //"cypress/require-data-selectors": "warn",
  "cypress/no-pause": "error"
  }
}
